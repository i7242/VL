<A "S" "Script">
<F "VrStringField">
"-- @type 3D

subject:setTrackedEntity(chasedObject)

local function computeVectorMeanValue(buffer, newValue, threshold)

	-- Clear the older values
	table.sort(buffer, function(a,b) return (a.date < b.date) end)
	-- Keep only the values which date are less old than the given threshold
	local newBuffer = {}
	local currentDate = Simulation:getElapsedTime()
	for k,v in ipairs(buffer) do
		if (currentDate - v.date) < threshold then table.insert(newBuffer,v) end
	end
	buffer = newBuffer

	-- Insert the current value
	table.insert(buffer, {date = currentDate, value = newValue} )

	local meanValue = Vec3(0, 0, 0)

	-- Compute the mean value
	for i = 1, #buffer do
		meanValue = buffer[i].value + meanValue
	end
	meanValue = meanValue * 1/(#buffer)

	return meanValue, buffer

end

local frontDirs = {}
local positions = {}

while true do

	meanFrontDir, frontDirs = computeVectorMeanValue(frontDirs, chasedObject:getStageOrientation().frontDirection, directionInertia)
	meanPosition, positions = computeVectorMeanValue(positions, chasedObject:getStagePosition(), positionInertia)

	local delta = meanFrontDir * distance
	delta.y = delta.y + upOffset
	subject:setStagePosition(meanPosition + delta)

	waitAndUpdate()

end
"
</F>
</A>
 
